<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>result_module_wk</id>
	<name>result_module_wk Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.result_module_wk</package>
	<author>Wuqu kawoq</author>
	<description>
		Link the result of a patient with AWS S3 Bucket
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/result_module_wk/update.rdf</updateURL>
	<require_version>1.8.2</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.result_module_wk.result_module_wkActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.result_module_wk.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		result_module_wk.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with result_module_wk. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

